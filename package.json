{
  "name": "version-buddy",
  "version": "0.1.0",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "doc-cookies": "^1.0.2",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-jss": "^8.6.1",
    "react-redux": "^5.0.6",
    "react-scripts": "^1.1.5",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "why-did-you-update": "^0.1.1"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "flow": "flow",
    "precommit": "lint-staged",
    "start": "REACT_APP_BUILD_TYPE=local HTTPS=true PORT=8443 react-scripts start",
    "start_windows": "set REACT_APP_BUILD_TYPE=local&&set HTTPS=true&&set PORT=8443&&react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!office-ui-fabric-react)/\" --env=jsdom",
    "test:ci": "react-scripts test --transformIgnorePatterns \"node_modules/(?!office-ui-fabric-react)/\" --env=jsdom --testResultsProcessor ./node_modules/jest-junit",
    "testcoverage": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-jest": "^22.4.3",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "flow-bin": "^0.63.1",
    "flow-typed": "^2.4.0",
    "husky": "^0.14.3",
    "jest-junit": "^5.0.0",
    "lint-staged": "^6.0.0",
    "moxios": "^0.4.0",
    "prettier": "^1.12.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.5.1",
    "source-map-explorer": "^1.6.0"
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "text"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "test/junit.xml",
    "classNameTemplate": "{classname} - {title}",
    "titleTemplate": "{classname} - {title}",
    "ancestorSeparator": " > ",
    "usePathForSuiteName": "true"
  }
}
